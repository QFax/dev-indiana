# Log level: INFO, DEBUG, WARN, ERROR
LOG_LEVEL=INFO

# Comma-separated list of Gemini API keys
GEMINI_API_KEYS=your_gemini_api_key_1,your_gemini_api_key_2

# Gemini API URL
GEMINI_API_URL=https://generativelanguage.googleapis.com

# Valkey connection details
VALKEY_HOST=valkey
VALKEY_PORT=6379
VALKEY_PASSWORD=

# Rate limiting (per minute)
RATE_LIMIT_PER_MINUTE=60
# Rate limit window: fixed or sliding
RATE_LIMIT_WINDOW=fixed

# Proxy authentication
PROXY_API_KEY=a_very_secure_proxy_key
# Allow using Gemini keys for auth: true or false
ALLOW_GEMINI_KEYS_FOR_AUTH=true